<div class="modal fade" id="deleteModal" tabindex="-1" role="dialog" aria-labelledby="deleteModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="modalTitle"></h5>
                <button type="button" class="close" data-dismiss="modal" id="xClose" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <p>This action can't be reversed</p>
            </div>
            <div class="modal-footer">
                <button id=modalDeleteBtn onclick="" type="button" class="btn btn-outline-danger">Delete</button>
                <button type="button" class="btn btn-outline-warning" id="buttonClose" data-dismiss="modal">Cancel</button>
            </div>
        </div>
    </div>
</div>
